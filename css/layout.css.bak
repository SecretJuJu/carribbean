/****************************/
.footer {width:100%; display:block;height:191px;}
.footer .bg1 .FT	{width:960px; height:45px; background:#e8e8e8; display:block; margin:0 auto;}
.footer .bg1 {width:100%; height:46px; background:#e8e8e8; display:block;}
.footer .bg1 .FT .SnS  {width:301px; height:45px}
.footer .bg1 .FT .SnS li {float:left}
.footer .bg1 .FT .Resort_fac,
.footer .bg1 .FT .Family_Site{float:right;}
.footer .bg1 .FT .Resort_fac {margin:-35px 194px 0}
.footer .bg1 .FT .Family_Site{margin-top:-35px}
.footer .bg1 .FT .Resort_fac .selector li{display:none;}
.footer .bg1 .FT .Family_Site .selector li {display:none}

.footer .bg2 {background:#434a54; width:100%; height:145px;position:relative}
.footer .bg2 .FM{width:960px;height:145px; margin:0 auto;}
.footer .bg2 .FM .menu {width:640px;height:35px;}
.footer .bg2 .FM .menu li {float:left; margin-top:15px}
.footer .bg2 .FM .menu li a {color:#8e8e8e;text-decoration: none;}
.footer .bg2 .FM .menu li:nth-child(3) a,
.footer .bg2 .FM .menu li:nth-child(4) a,
.footer .bg2 .FM .menu li:nth-child(6) a,
.footer .bg2 .FM .menu li:nth-child(7) a {color:#cfcfcf}
.footer .bg2 .FM .menu li a:after {content: " | ";display: inline-block;width: 10px;color: #8e8e8e;
font-size: 1px;line-height: 18px;margin-left: 10px;}
.footer .bg2 .FM .Explain {width: 685px; display:inline-block}
.footer .bg2 .FM .Explain p {height: 20px;color: #8e8e8e;line-height: 20px; font-size:13px; font-weight:300;}
.footer .bg2 .FM .Explain p:nth-child(2) a{text-decoration: none;color: #8e8e8e; line-height:20px;}
.footer .bg2 .FM .Explain p:nth-child(3) a{text-decoration: none;color: #8e8e8e; font-weight:bold; line-height: 20px;}
.footer .bg2 .FM .Info_img {display:inline-block; width:110px;height:58px;margin:-35px 0 22px;}






