
.header {width:100%;height:162px;}
.header a:hover{text-decoration: none;}
.header {text-align:center;border-bottom:1px solid #c4c4c4;}
.header .part1{width:100%;background:#f5f5f5;height:38px;border-bottom:1px solid #c4c4c4;}
.header .menu {margin:0 auto; width:920px;}
.header .menu li {display:inline-block;float:left;height:38px;}

.header .menu li:nth-child(1){padding-right:30px;}

.header .menu .SelectBox {padding-top:9px}

.header .menu li:nth-child(5) img {margin:12px 9px 12px 121px}
.header .menu .langs {display:none}
.header .Tab2 form li a span{font-family: 'µ¸¿ò',"dotum",sans-serif; color:#828282}
.header .Tab2 {width:100%;height:65px;top:38px;margin-top:14px;}
.header .Tab2 .part2 {width:920px; height:65px; margin:0 auto;}
.header .Tab2 form{ width:920px;height:65px; float:left;}
.header .Tab2 .input {border:1px solid #d9d9d9; width:242px;height:32px;margin-left:-20px;}
.header .Tab2 li { float:left;color: #828282;}
.header .Tab2 .searchArea {margin:4px 0 0 0; width:200px;height:21px;}
.header .Tab2  li:nth-child(2){margin-left:120px;}
.header .Tab2 li {margin-left: 20px;}
.header .Tab2 li:nth-child(3){margin-left:115px;}

.header .part3 {width:100%; height:42px;text-align:center;border-top:2px solid #008ee6;}
.header .part3 .gnbs {width:960px; height:43px; margin:0 auto;}
.header .part3 .gnbs div {float:left; width:240px;height:43px;}
.header .part3 .gnbs div ul {position: relative; margin:43px 0 0; height:294px;width:239px;background:#fff;
border-left:1px solid #f0f0f0;}

.header .part3 .gnbs div:nth(4) {width:238px; border-right:1px solid #f0f0f0;}
.header .part3 .gnbs div ul li {display:inline-block; width:218px; height:28px}
.header .part3 .gnbs div ul li a {font-size:15px; color:#666}
.header .part3 .gnbs div div a{display:inline-block;width:240px; height:43px;line-height:43px;
font-size: 18px;font-weight:400;}
.header .part3 .gnbs div div a:hover{text-decoration:none; background:#18a5ff; color:#fff}



