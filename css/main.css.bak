.cont .Info .cont_inner .info_inner .Etc ul li:nth-child(5){width:323px}
.cont .Info .cont_inner .info_inner .Etc ul li:nth-child(1),
.cont .Info .cont_inner .info_inner .Etc ul li:nth-child(3){border-right:0}
.cont .Info .cont_inner .info_inner .Etc ul li a{text-align:center;display: inline-block;
width: 160px;height: 80px; line-height:80px;color: #272830;font-size: 16px;}
/****  traffic 작업 ******/
.cont .Info .cont_inner .info_inner .traffic {background:#fff;padding:0 0 0 20px;}
.cont .Info .cont_inner .info_inner .traffic span{color: #3b3b3b;font-size: 15px;
font-weight:400;font-family:"Noto Sans KR", Montserrat, "Malgun Gothic", Dotum, sans-serif;
letter-spacing: -1px; margin:0 10px 0}
.cont .Info .cont_inner .info_inner .traffic img{margin-top:20px}
.cont .Info .cont_inner .info_inner .traffic strong{color: #6d2cb7;font-size: 17px;
