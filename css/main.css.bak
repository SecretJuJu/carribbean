.cont .Info {background:#e9f4f7;width:100%;height:737px;display:block;padding: 60px 0;}
.cont .Info .cont_inner {width:1000px;height:617px;margin:0 auto;}
.cont .Info .cont_inner .name {position: relative;margin: 0 0 40px; text-align:center;}
.cont .Info .cont_inner .name strong{display: inline-block;position:font-size:35px; ;z-index: 5;margin: 0 0 10px;
width:379px;color:#272830;font-size:38px;font-weight:1000;
font-family: Montserrat, sans-serif;line-height:1;}
.cont .Info .cont_inner .name span{color: #272830;font-size:20px;font-weight: 300;}
.cont .Info .cont_inner .info_inner {position: relative;width:1000px;height:500px;}

.cont .Info .cont_inner .info_inner .News{width:325px; height:498px; background:#fff;
float:left;border:1px solid #e6e6e6;border-bottom:2px solid #e6e6e6;
border-top:2px solid #1450f5;box-sizing:border-box;padding:20px}

.cont .Info .cont_inner .info_inner .T_Time{width:325px; height:498px; background:#fff;
float:left;border:1px solid #e6e6e6;border-bottom:2px solid #e6e6e6;
border-top:2px solid #1450f5; margin-left:10px;box-sizing:border-box;
padding:35px 20px 35px}

.cont .Info .cont_inner .info_inner .Etc{width:325px; height:378px; background:#fff;
float:left; margin-left:10px;box-sizing:border-box;}

.cont .Info .cont_inner .info_inner .traffic{width:325px;height:109px; background:#fff;
float:left; margin:10px 0 0 10px; box-sizing:border-box;}

/* New 작업 */
.cont .Info .cont_inner .info_inner .News .M_tit {display:block;margin: 0 0 5px;
color: #272830;font-size:28px;font-weight:800;font-family:Montserrat,sans-serif;
line-height:1; text-align:center;}
.cont .Info .cont_inner .info_inner .News .S_tit{text-align:center;
color: #272830;font-size: 18px;font-weight: 300;}

.cont .Info .cont_inner .info_inner .News ul {margin-top:35px;border-top:1px solid black;
border-bottom:1px solid black; list-style:circle;}
.cont .Info .cont_inner .info_inner .News ul li {height: 48px;border-bottom:1px solid #ebebeb;}
.cont .Info .cont_inner .info_inner .News a {display:block; position: relative;
overflow:hidden;padding:16px 10px;width: 100%; white-space:nowrap; line-height:1;
font-size:15px; text-overflow:ellipsis; color:#666666}
.cont .Info .cont_inner .info_inner .News a:hover{text-decoration:none}
.cont .Info .cont_inner .info_inner .News ul li:nth-child(7){border-bottom:0}

/* New 끝*/
/* T_Time */

.cont .Info .cont_inner .info_inner .T_Time .M_tit{position: relative;margin: 0 0 33px;
text-align: center;display:block;color: #272830;
font-size: 28px;font-weight: 800;font-family: Montserrat, sans-serif;line-height: 1;
text-align:center;}
